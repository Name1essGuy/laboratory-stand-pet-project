###############################################################
# Это конфигурационный файл Grafana.                          #
# Все настройки закомментированы и используют значения        #
# по умолчанию.                                              #
# Полная документация: https://grafana.com/docs/grafana/latest/administration/configuration/
###############################################################

[paths]
# data = /var/lib/grafana
# logs = /var/log/grafana
# plugins = /var/lib/grafana/plugins
# provisioning = conf/provisioning

[server]
protocol = http
http_port = {{ grafana_port | default("3000") }}
domain = {{ grafana_host | default("localhost") }}
root_url = %(protocol)s://%(domain)s:%(http_port)s/
# enable_gzip = false
# static_root_path = public

[database]
# type = sqlite3
# path = grafana.db
# cache_mode = private

[session]
# provider = file
# provider_config = sessions
# cookie_name = grafana_sess
# cookie_secure = false
# session_life_time = 86400

[analytics]
# reporting_enabled = true
# check_for_updates = true

[security]
admin_user = {{ grafana_admin_username | default("admin") }}
admin_password = {{ grafana_admin_password | default("admin") }}
# secret_key = SW2YcwTIb9zpOOhoPsMm
# login_remember_days = 7
# cookie_secure = false

[users]
# allow_sign_up = true
# auto_assign_org = true
# auto_assign_org_role = Viewer

[auth]
# login_cookie_name = grafana_session

[auth.anonymous]
# enabled = false
# org_name = Main Org.
# org_role = Viewer

[auth.basic]
# enabled = true

[log]
# mode = console
# level = info
# filters =

[metrics]
# enabled = true
# interval_seconds = 10

[snapshots]
# external_enabled = true
# external_snapshot_url = https://snapshots-origin.grafana.com
# external_snapshot_name = Publish to snapshot.grafana.com

[alerting]
# enabled = true
# execute_alerts = true

[unified_alerting]
# enabled = false

[explore]
# enabled = true

[panels]
# disable_sanitize_html = false

[plugins]
# allow_loading_unsigned_plugins =
# enable_alpha = false